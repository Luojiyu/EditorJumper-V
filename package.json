{
  "name": "editorjumper",
  "displayName": "EditorJumper",
  "description": "A VS Code extension that allows you to seamlessly jump between JetBrains IDEs and other popular code editors (such as VS Code, Cursor, Trae, and Windsurf). It maintains your cursor position and editing context, greatly improving development efficiency in multi-editor environments.",
  "version": "1.1.0",
  "publisher": "wanniwa",
  "repository": {
    "type": "git",
    "url": "https://github.com/wanniwa/EditorJumper-V"
  },
  "bugs": {
    "url": "https://github.com/wanniwa/EditorJumper-V/issues"
  },
  "homepage": "https://github.com/wanniwa/EditorJumper-V#readme",
  "engines": {
    "vscode": "^1.81.0"
  },
  "icon": "image/pluginIcon.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "intellij",
    "idea",
    "jetbrains",
    "jump",
    "open",
    "editor",
    "cursor",
    "trae",
    "windsurf"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "editorjumper.openInJetBrains",
        "title": "Open in JetBrains IDE"
      },
      {
        "command": "editorjumper.selectJetBrainsIDE",
        "title": "Select JetBrains IDE"
      },
      {
        "command": "editorjumper.configureIDE",
        "title": "Configure EditorJumper"
      }
    ],
    "keybindings": [
      {
        "command": "editorjumper.openInJetBrains",
        "key": "shift+alt+o"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "editorjumper.openInJetBrains",
          "group": "navigation",
          "when": "editorTextFocus"
        }
      ],
      "explorer/context": [
        {
          "command": "editorjumper.openInJetBrains",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "EditorJumper",
      "properties": {
        "editorjumper.selectedIDE": {
          "type": "string",
          "default": "IDEA",
          "description": "Currently selected JetBrains IDE"
        },
        "editorjumper.ideConfigurations": {
          "type": "array",
          "default": [
            {
              "name": "IDEA",
              "isCustom": false,
              "hidden": false,
              "commandPath": null
            },
            {
              "name": "WebStorm",
              "isCustom": false,
              "hidden": false,
              "commandPath": null
            },
            {
              "name": "PyCharm",
              "isCustom": false,
              "hidden": false,
              "commandPath": null
            },
            {
              "name": "GoLand",
              "isCustom": false,
              "hidden": false,
              "commandPath": null
            },
            {
              "name": "CLion",
              "isCustom": false,
              "hidden": false,
              "commandPath": null
            },
            {
              "name": "PhpStorm",
              "isCustom": false,
              "hidden": false,
              "commandPath": null
            },
            {
              "name": "RubyMine",
              "isCustom": false,
              "hidden": false,
              "commandPath": null
            },
            {
              "name": "Rider",
              "isCustom": false,
              "hidden": false,
              "commandPath": null
            },
            {
              "name": "Android Studio",
              "isCustom": false,
              "hidden": false,
              "commandPath": null
            }
          ],
          "description": "Configured JetBrains IDEs",
          "items": {
            "type": "object",
            "required": [
              "name",
              "isCustom",
              "hidden"
            ],
            "properties": {
              "name": {
                "type": "string",
                "description": "Display name for the IDE"
              },
              "isCustom": {
                "type": "boolean",
                "description": "Whether this is a custom IDE",
                "default": false
              },
              "hidden": {
                "type": "boolean",
                "description": "Whether to hide this IDE in the selection menu",
                "default": false
              },
              "commandPath": {
                "type": ["string", "null"],
                "description": "Command or path to open the IDE"
              }
            }
          }
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test",
    "package": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "~20.17.24",
    "@types/vscode": "^1.81.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.3.0",
    "eslint": "^9.22.0"
  }
}
